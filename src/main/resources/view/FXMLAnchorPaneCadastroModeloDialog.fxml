         </children>
      </GridPane>
      <Button fx:id="btConfirmar" layoutX="424.0" layoutY="362.0" mnemonicParsing="false" onAction="#handleBtConfirmar" text="Confirmar" />
      <Button fx:id="btCancelar" layoutX="507.0" layoutY="362.0" mnemonicParsing="false" onAction="#handleBtCancelar" text="Cancelar" />
